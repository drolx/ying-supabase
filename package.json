{
  "name": "ying-supabase",
  "private": true,
  "type": "module",
  "scripts": {
    "jsr": "bunx jsr",
    "build": "nuxt build",
    "dev": "nuxt dev --port 3067",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "rimraf node_modules bun.* dist/*",
    "sup": "supabase",
    "sup:funcs": "supabase functions serve --env-file ./.env.edge-functions --no-verify-jwt",
    "sup:diff": "supabase db diff --local --use-migra -f initial",
    "sup:seed-pull": "rimraf supabase/seeds/*; supabase db dump --local --data-only > supabase/seeds/initial.sql",
    "sup:seed": "bun sup seed --local",
    "sup:sync": "bun sup db reset --local",
    "migration": "bun sup migration",
    "sup:mig-reset": "rimraf supabase/migration/*; bun migration new initial",
    "sup:mig-up": "bun migration up --local",
    "sup:login": "bun sup login",
    "sup:logout": "bun sup logout",
    "sup:status": "bun sup status",
    "sup:start": "bun sup start --ignore-health-check",
    "sup:stop": "bun sup stop",
    "sup:delete": "bun sup:stop --no-backup",
    "sup:test": "bun sup test",
    "sup:inspect": "bun sup inspect",
    "sup:types": "bun sup gen types --local --lang=typescript --schema public > supabase/functions/database.types.ts",
    "gen:types": "bun sup:types; better-supabase-types --singular -i supabase/functions/database.types.ts -o supabase/functions/types.d.ts",
    "db:gen-rls": "supabase-drizzle generate --config ./drizzle.config.ts",
    "db:gen": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:sync": "drizzle-kit migrate --config ./drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./drizzle.config.ts",
    "db:seed": "bun supabase/seed.ts",
    "start": "bun sup:start; bun db:seed; bun gen:types",
    "reset": "rimraf supabase/seeds/* supabase/migrations/*; bun sup:delete; bun sup:start; bun db:gen; bun db:sync; bun db:seed; bun gen:types"
  },
  "dependencies": {
    "@nuxtjs/supabase": "^1.4.2",
    "@pinia/nuxt": "^0.7.0",
    "@supabase/functions-js": "^2.4.3",
    "@supabase/supabase-js": "^2.46.1",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^11.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "npm:drizzle-orm@0.36.2",
    "npm:drizzle-orm": "npm:drizzle-orm@0.36.2",
    "nuxt": "^3.14.159",
    "pinia": "^2.2.6",
    "postgres": "^3.4.5",
    "vue": "latest",
    "vue-router": "latest",
    "vuetify-nuxt-module": "^0.18.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@unocss/nuxt": "^0.64.0",
    "better-supabase-types": "^2.7.6",
    "drizzle-kit": "^0.28.0",
    "rimraf": "^6.0.1",
    "supabase": "^1.219.2",
    "unocss": "^0.64.0"
  }
}
