    "db:seed": "bun sup seed",
    "db:sync": "bun sup db reset",
    "migration": "bun sup migration new",
    "migration:reset": "rimraf supabase/migration/* && bun migration initial",
    "migration:up": "bun migration up",
    "sup:login": "bun sup login",
    "sup:logout": "bun sup logout",
    "sup:status": "bun sup status",
    "start": "bun sup start",
    "stop": "bun sup stop",
    "sup:test": "bun sup test",
    "inspect": "bun sup inspect",
    "gen": "bun sup gen"
  },
  "dependencies": {
    "@nuxtjs/supabase": "^1.4.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "nuxt": "^3.14.159",
    "postcss": "^8.4.47",
    "sass": "^1.80.6",
    "vue": "latest",
    "vue-router": "latest",
    "vuetify": "^3.7.0-beta.1",
    "vuetify-nuxt-module": "^0.18.3"
  },
  "devDependencies": {
    "@unocss/nuxt": "^0.64.0",
    "rimraf": "^6.0.1",
    "supabase": "^1.219.2",
    "unocss": "^0.64.0"
  }
