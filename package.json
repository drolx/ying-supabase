{
  "name": "ying-supabase",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "rimraf node_modules bun.* dist/*",
    "sup": "supabase",
    "db": "bun sup db",
    "db:diff": "supabase db diff --local --use-migra -f initial_schema",
    "db:seed-pull": "rimraf supabase/seeds/*; supabase db dump --local --data-only > supabase/seeds/initial.sql",
    "db:seed": "bun sup seed --local",
    "db:sync": "bun sup db reset --local",
    "migration": "bun sup migration",
    "migration:reset": "rimraf supabase/migration/*; bun migration new initial_schema",
    "migration:up": "bun migration up --local",
    "sup:login": "bun sup login",
    "sup:logout": "bun sup logout",
    "sup:status": "bun sup status",
    "sup:start": "bun sup start",
    "sup:stop": "bun sup stop",
    "sup:test": "bun sup test",
    "sup:inspect": "bun sup inspect",
    "sup:types": "bun sup gen types --local --lang=typescript --schema public --schema auth"
  },
  "dependencies": {
    "@nuxtjs/supabase": "^1.4.2",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^11.2.0",
    "nuxt": "^3.14.159",
    "vue": "latest",
    "vue-router": "latest",
    "vuetify-nuxt-module": "^0.18.3"
  },
  "devDependencies": {
    "@unocss/nuxt": "^0.64.0",
    "rimraf": "^6.0.1",
    "supabase": "^1.219.2",
    "unocss": "^0.64.0"
  }
}
