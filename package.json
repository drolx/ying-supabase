{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "rimraf node_modules bun.* dist/*",
    "sup": "supabase",
    "db": "bun sup db",
    "db:diff": "supabase db diff my_schema",
